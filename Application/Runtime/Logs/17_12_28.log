SQL: INSERT INTO `sxkj_kaifeng_cs` (`title`,`content`) VALUES ('开封市科学技术局研究出台科学技术评价专家管理办法','<p style=\"text-indent:2em;\">为加强科学技术评价工作的管理，充分发挥科技专家（以下简称专家）在科技评价活动中的作用，规范各类科技管理活动中的科技评价工作，确保科技评价过程的科学、公正、公平，开封市科技局研究制定了《开封市科学技术局科学技术评价专家管理办法（试行）》（汴科文〔2017〕196号）（以下简称《办法》）。</p>\n<p style=\"text-indent:2em;\">《办法》对开封市科技局的科技评价专家库（以下简称专家库）提出了具体的入库、管理、使用要求。专家库是由各行业技术、管理、财务等专家组成，为我市科技管理工作提供决策咨询服务的专家管理信息系统。</p>\n<p style=\"text-indent:2em;\">专家库按照“自愿申请、单位推荐、科技主管部门认定”的原则，面向市内外高等院校、科研机构、企业、市级及以上行业协会、政府决策咨询机构等单位，采取集中入选和日常补选相结合的方式征集。</p>\n<p style=\"text-indent:2em;\">此外，《办法》还建立了专家信用评价制度，对年度信用综合评价为不合格的专家，不再列入专家库。</p>\n<p><br></p>\n<p style=\"text-indent:2em;text-align:right;\">资源计划科</p>\n<p style=\"text-indent:2em;text-align:right;\">2017年12月21日</p>') [ RunTime:0.026000s ]
