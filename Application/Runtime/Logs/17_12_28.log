SQL: INSERT INTO `sxkj_kaifeng_cs` (`title`,`content`) VALUES ('北斗众创空间成功晋级省级众创空间','<p style=\"text-indent:2em;\">近期,2017年度河南省众创空间备案单位名单公布，在全省58家备案企业中北斗众创空间榜上有名,是我市今年唯一省级备案的众创空间。北斗众创空间立足于于生态文明建设、循环经济、节能环保等基本国策，以技术研发为核心，以科技成果转化为目标，以实验和孵化平台为载体，联合高校、科研院所共建的集创业服务、创业办公、创业培训、科研试验、技术转移、公益科普于一体的公共服务平台。近年来，开封市委、市政府深入实施创新驱动发展战略，优化创新创业环境。北斗众创空间省级备案成功，将为顺河区实施创新驱动发展战略和经济发展发挥重要引擎作用，带动全区产学研用工作再上新台阶。</p>\n<p><br></p>\n<p><br></p>') [ RunTime:0.026000s ]
