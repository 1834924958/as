SQL: INSERT INTO `sxkj_kaifeng_cs` (`title`,`content`) VALUES ('2016年度开封市企业研究开发财政补助公示','<p>    为引导企业普遍建立研发准备金制度，有计划、持续地增加研发投入，根据河南省财政厅、科技厅等6部门联合印发的《河南省企业研究开发财政补助实施方案（试行）》（豫财科【2017】166号）和《关于组织开展企业研究开发财政补助的通知》（豫财科【2017】190号）文件精神，在企业自愿申报的基础上，经开封市科技、国税、地税、统计等部门审核，拟对2016年开封市28家企业企业研发经费给予财政补助（名单附后），现予以公示，公示期为5天。公示期间如有异议，请书面反馈市财政局、市科技局。</p>\n<p>    开封市财政局联系人：曾庆利电话：23876033</p>\n<p>    开封市科技局联系人：邬建飞电话：25957056</p>\n<p><br></p>\n<p><br></p>\n<p><a href=\"http://www.kfst.cn/govermentDetail/158567\" target=\"_self\" title=\"附件：河南省企业研发费用财政补助汇总表\" style=\"color:#1f497d;text-decoration:underline;\"><span style=\"color:#1f497d;\">附件：河南省企业研发费用财政补助汇总表</span></a></p>\n<p><br></p>\n<p><br></p>\n<p style=\"text-align:right;\">开封市财政局开封市科技局</p>\n<p style=\"text-align:right;\">                                                                                       2017年12月6日</p>\n<p><br></p>') [ RunTime:0.032000s ]
