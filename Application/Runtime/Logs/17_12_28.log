SQL: INSERT INTO `sxkj_kaifeng_cs` (`title`,`content`) VALUES ('杞县1家企业获批国家第二批星创天地','<p style=\"text-indent:2em;line-height:2em;\"><span style=\"text-indent:2em;\">2017年11月，国家科技部公布了2017年度第二批星创天地名单。杞县依托河南省农投电子商务有限公司组建“汴州农业科技星创天地”获批科技部第二批星创天地。这是开封市2017年唯一家获批国家级星创天地。“星创天地”是农业农村领域的众创空间，是新型农业创新创业一站式开放性综合服务平台。“汴州农业科技星创天地”获批国家级星创天地，对于进一步激发杞县农村农业创新创业活力，优化农村创新创业环境，加快科技成果转移转化，提高农业创新供给质量和产业竞争力,培育新型农业经营主体，以创业带动就业起到积极作用。</span><br></p>\n<p><br></p>\n<p><br></p>\n<p style=\"text-align:right;line-height:2em;\">杞县科学技术局</p>\n<p style=\"text-align:right;line-height:2em;\">2017年12月1日</p>\n<p><br></p>\n<p><br></p>') [ RunTime:0.267000s ]
