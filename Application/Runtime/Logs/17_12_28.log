SQL: INSERT INTO `sxkj_kaifeng_cs` (`title`,`content`) VALUES ('关于对开封市科技计划项目管理办法（试行）等三个文件公开征求意见的通知','<p style=\"text-indent:2em;text-align:left;line-height:2em;\"><span style=\"font-family:微软雅黑, \'microsoft yahei\';\">《开封市科技计划项目管理办法（试行）》《开封市科技创新平台建设与管理办法（试行）》《开封市科学技术评价专家管理办法（试行）》三个文件的征求意见稿已完成起草工作，现面向全市公开征集意见建议。有修改意见建议者，请于12月15日前书面反馈开封市科技局资源计划科。</span></p>\n<p style=\"text-indent:2em;text-align:left;line-height:2em;\"><span style=\"font-family:微软雅黑, \'microsoft yahei\';\">联系人：邬建飞</span></p>\n<p style=\"text-indent:2em;text-align:left;line-height:2em;\"><span style=\"font-family:微软雅黑, \'microsoft yahei\';\">联系电话：25957056</span></p>\n<p style=\"line-height:2em;\"><a href=\"http://www.kfst.cn/govermentDetail/158722\" target=\"_self\" title=\"附件\" style=\"color:#1f497d;text-decoration:underline;\"><span style=\"color:#1f497d;font-family:微软雅黑, \'microsoft yahei\';\">附件</span></a></p>\n<p style=\"text-indent:2em;text-align:right;line-height:2em;\"><span style=\"font-family:微软雅黑, \'microsoft yahei\';\">2017年12月8日        </span></p>\n<p><br style=\"text-indent:2em;text-align:left;\"></p>') [ RunTime:0.030000s ]
