SQL: INSERT INTO `sxkj_kaifeng_cs` (`title`,`content`) VALUES ('关于“开封市碳酸二甲酯新型催化剂材料创新型科技团队” 认定结果的公示','<p style=\"line-height:1.75em;text-indent:2em;\">根据2017年12月5日局党组会议精神，市科技局组织专家对依托开封市中科新材料有限公司的市级碳酸二甲酯新型催化剂材料创新型科技团队进行评审，拟认定为2017年度开封市创新型科技团队，现予公示。自即日起5日内持有异议的单位或个人均可提出。提出异议的单位或个人须具实名提供书面异议材料，并提供必要的证明材料。公示期为5天，自公布之日起计算。</p>\n<p style=\"line-height:1.75em;text-indent:2em;\"><span style=\"text-indent:2em;\"></span></p>\n<p style=\"text-indent:2em;\">公示电话：0371-25955246     </p>\n<p style=\"text-indent:2em;\">纪检监察室电话：25957981</p>\n<p style=\"text-indent:2em;\">邮    箱：HNKFKW@QQ.com    </p>\n<p style=\"text-indent:2em;\">开封市科技局 </p>\n<p style=\"text-indent:2em;\">2017年12月13日</p>\n<p style=\"line-height:1.75em;text-indent:2em;\"><span style=\"text-indent:2em;\"></span><br></p>') [ RunTime:0.032000s ]
